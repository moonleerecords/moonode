<p id="notice"><%= notice %></p>

<h1>Listing Releases</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Catalognumber</th>
      <th>releaseDate</th>
      <th>Description</th>
      <th>Tracklist</th>
      <th>Bandcampalbumid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @releases.each do |release| %>
      <tr>
        <td><%= release.title %></td>
        <td><%= release.catalogNumber %></td>
        <td><%= release.releaseDate %></td>
        <td><%= release.description %></td>
        <td><%= release.tracklist %></td>
        <td><%= release.bandcampAlbumId %></td>
        <td><%= link_to 'Show', release %></td>
        <td><%= link_to 'Edit', edit_release_path(release) %></td>
        <td><%= link_to 'Destroy', release, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Release', new_release_path %>
