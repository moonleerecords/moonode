<p id="notice"><%= notice %></p>

<h1>Listing Artist Members</h1>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Member</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artist_members.each do |artist_member| %>
      <tr>
        <td><%= artist_member.Artist %></td>
        <td><%= artist_member.Member %></td>
        <td><%= artist_member.role %></td>
        <td><%= link_to 'Show', artist_member %></td>
        <td><%= link_to 'Edit', edit_artist_member_path(artist_member) %></td>
        <td><%= link_to 'Destroy', artist_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist member', new_artist_member_path %>
